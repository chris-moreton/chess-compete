# SPSA Worker Configuration
# Copy this file to .env and fill in your values

# API endpoint (usually don't need to change)
SPSA_API_URL=https://chess-compete-production.up.railway.app

# Your API key (get from server admin)
SPSA_API_KEY=your-api-key-here

# Worker identification (optional, helps identify this worker in logs)
COMPUTER_NAME=docker-worker-1

# Number of parallel games per worker container
CONCURRENCY=4
